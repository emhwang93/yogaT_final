<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Worst3">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/floralwhite">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="?attr/colorPrimary"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="0dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Worst 3"
                android:fontFamily="@font/cafe24danjunghae"
                android:layout_gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:textSize="30dp"
                android:textColor="@color/dimgray">
            </TextView>

        </androidx.appcompat.widget.Toolbar>

        <EditText
            android:id="@+id/dateWorst3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="날짜 선택:"
            android:fontFamily="@font/gmarketsansttflight"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:background="@android:color/transparent"
            android:textSize="15sp"
            android:textColor="@color/darkgray"
            android:layout_marginTop="80dp"
            android:layout_marginLeft="165dp">
        </EditText>

        <ImageView
            android:id="@+id/datepickerWorst3"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="65dp"
            android:src="@drawable/calendar2"
            android:layout_toRightOf="@+id/dateWorst3"/>

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="200dp"
            android:layout_height="220dp"
            android:layout_marginTop="150dp"
            android:layout_marginLeft="5.5dp"
            android:src="@drawable/image_border"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="122dp"
            tools:src="@tools:sample/avatars" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="200dp"
            android:layout_height="220dp"
            android:layout_marginTop="340dp"
            android:layout_marginLeft="210dp"
            android:src="@drawable/image_border"
            tools:layout_editor_absoluteX="231dp"
            tools:layout_editor_absoluteY="313dp"
            tools:src="@tools:sample/avatars" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="200dp"
            android:layout_height="220dp"
            android:layout_marginLeft="5.5dp"
            android:layout_marginTop="530dp"
            android:src="@drawable/image_border"
            tools:layout_editor_absoluteX="80dp"
            tools:layout_editor_absoluteY="518dp"
            tools:src="@tools:sample/avatars" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="자세이름"
            android:textColor="@color/dimgray"
            android:textSize="15dp"
            android:layout_marginTop="505dp"
            android:layout_marginLeft="10sp"
            tools:layout_editor_absoluteX="81dp"
            tools:layout_editor_absoluteY="272dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="340dp"
            android:layout_marginTop="321dp"
            android:text="자세이름"
            android:textColor="@color/dimgray"
            android:textSize="15dp"
            tools:layout_editor_absoluteX="263dp"
            tools:layout_editor_absoluteY="275dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="20dp"
            android:text="1"
            android:fontFamily="@font/cafe24ssurround"
            android:textColor="@color/dimgray"
            android:textSize="30sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="117dp"
            tools:layout_editor_absoluteY="659dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="130dp"
            android:text="자세이름"
            android:textColor="@color/dimgray"
            android:layout_marginLeft="10dp"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="370dp"
            android:layout_marginTop="290dp"
            android:fontFamily="@font/cafe24ssurround"
            android:textColor="@color/dimgray"
            android:text="2"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:fontFamily="@font/cafe24ssurround"
            android:textColor="@color/dimgray"
            android:textSize="30sp"
            android:textStyle="bold"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="475dp"
            app:layout_constraintStart_toStartOf="parent" />



    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>